{
  "name": "webpack-starter-kit",
  "version": "0.0.1",
  "description": "A starter kit for React and Redux applications.",
  "main": "./src/index.js",
  "repository": "https://github.com/mattcarlotta/webpack-starter",
  "author": "Matt Carlotta",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "scripts": {
    "analyze": "ANALYZE=true npm run build",
    "build": "rimraf dist && NODE_ENV=production webpack -p",
    "dev": "NODE_ENV=development PORT=3000 webpack-dev-server",
    "format": "prettier --write 'src/**/*.js'",
    "lint:js": "eslint 'src/**/*.js' --fix",
    "lint:styles": "stylelint **/*.scss",
    "start": "HOST=http://localhost: NODE_ENV=production PORT=8080 node server.js",
    "test": "NODE_ENV=testing jest --config ./jest.json --passWithNoTests",
    "test:cov": "NODE_ENV=testing jest --config ./jest.json --coverage",
    "test:watch": "NODE_ENV=test jest --config ./jest.json --watch"
  },
  "dependencies": {
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.11.0",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "babel-preset-env": "^1.7.0",
    "browserslist": "^4.14.5",
    "copy-webpack-plugin": "^6.2.0",
    "css-loader": "^4.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "error-overlay-webpack-plugin": "^0.4.1",
    "eslint": "^7.10.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.3",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-watch": "^7.0.0",
    "file-loader": "^6.1.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^4.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "jest-css-modules-transform": "^4.0.1",
    "jest-enzyme": "^7.1.2",
    "jest-haste-map": "^26.3.0",
    "jest-resolve": "^26.4.0",
    "jest-transform-stub": "^2.0.0",
    "jsdom": "^16.4.0",
    "mini-css-extract-plugin": "^0.11.3",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "prettier": "^2.1.2",
    "react-dev-utils": "^10.2.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^10.0.2",
    "style-loader": "^1.2.1",
    "stylelint": "^13.7.2",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-scss": "^3.18.0",
    "terser-webpack-plugin": "^4.2.2",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-manifest-plugin": "^2.2.0",
    "webpackbar": "^4.0.0"
  }
}
